#ceil of 2.3 is 3.0
#ceil of 3.8 is 4.0
#ceil of -2.3 is -2.0
#ceil of -3.8 is -3.0
def ceil(num):
    if num>0:
        num=int(num)+1
    else:
        num=int(num)
    return num

