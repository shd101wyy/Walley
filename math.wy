def matrix(row,col)    
    x=[] 
    y=[]
    i=0
    j=0
    while i<row:
        y=[]
        j=0
        while j<col:
            y.append(0)
            j=j+1
        x.append(y)
        i=i+1
    return x
#end define matrix

